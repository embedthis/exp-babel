{
    "name": "exp-babel",
    "description": "ES6 Compile for Expansive using babel",
    "version": "0.2.5",
    "keywords": [
        "expansive-plugin",
        "babel"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-babel.git"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "pak": ">=0.11.3"
    },
    "engines": {
        "node": ">= 0.4.0"
    },
    "pak": {
        "scripts": {
            "postcache": {
                "script": "npm('install -g', 'babel-cli')"
            },
            "install": {
                "script": "npm('install', 'babel-preset-es2015')"
            }
        }
    }
}
